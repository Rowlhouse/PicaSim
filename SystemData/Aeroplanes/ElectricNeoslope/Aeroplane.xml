<!--
Electric Neoslope
-->


<Misc
  launchOffsetX="2.0"
  launchOffsetY="0.0"
  launchOffsetZ="0.2"
  launchSpeed="6.0"
/>
<Physics>
  <Dynamics
    wingSpan="2.0"
    wingChord="0.25"
    CMRollFromY="0.02"
    friction="0.5"
    frictionScaleX="1"
    frictionScaleY="2"
    frictionScaleZ="2"
    />

  <Aerodynamics>
    <!-- Positions are in the centre of the chord and height, but at one end of the span (so extents.y may be negative). 
    Note that X is forward, Y is left and Z is up-->

    <Wing
      name="Mid"
      aerofoil="SystemData/Aerofoils/SemiSymmetric/RG14"
      numSections="1"
      mass="0.05"

      positionX="-0.06"
      positionY="-0.1"
      positionZ="0.01"
      pitch="-0.5"
      extentsX="0.22"
      extentsY="0.2"
      extentsZ="0.01"
      wingAspectRatio="10.0"
      groundEffect="1"
    />

    <Wing 
      name="Left1"
      aerofoil="SystemData/Aerofoils/SemiSymmetric/RG14"
      numSections="2"
      mass="0.35"

      positionX="-0.06"
      positionY="0.1"
      positionZ="0.01"
      roll="2"
      pitch="0"
      extentsX="0.21"
      extentsY="0.3"
      extentsZ="0.01"
      wingAspectRatio="10.0"
      groundEffect="1"

      CLPerDegree="0.022"
      CDPerDegree="0.02"
      CMPerDegree="-0.01"
      flapFraction="0.32"
      degreesPerControl="30"
      
      controlPerChannel6="0.1"
      />

    <Wing
      name="Left2"
      aerofoil="SystemData/Aerofoils/SemiSymmetric/RG14"
      numSections="2"
      mass="0.05"
      positionX="-0.07"
      positionY="0.4"
      positionZ="0.03"
      roll="3"
      pitch="1"
      extentsX="0.14"
      extentsY="0.6"
      extentsZ="0.01"      
      wingAspectRatio="10.0"
      groundEffect="1"

      CLPerDegree="0.022"
      CDPerDegree="0.02"
      CMPerDegree="-0.01"
      flapFraction="0.32"
      degreesPerControl="22"
      
      controlPerChannel0="1"
      controlDifferentialPerChannel0="-0.1"
      controlPerChannel6="0.1"
      />

    <Wing
      name="Right1"
      copy="Left1"
      mirror="1"
    />

    <Wing
      name="Right2"
      copy="Left2"
      mirror="1"
    />

    <Wing
      name="Tail"
      aerofoil="SystemData/Aerofoils/Symmetric/NACA0009"
      numSections="2"
      mass="0.045"

      positionX="-0.8"
      positionY="-0.2"
      positionZ="0.19"
      rotationX="0.0"
      rotationY="0.2"
      rotationZ="0.0"
      extentsX="0.10"
      extentsY="0.4"
      extentsZ="0.01"
      wingAspectRatio="5.0"
      groundEffect="1"
      
      washFromWingName1="Left1"
      washFromWingName2="Right1"
      washFromWingFraction1="0.5"
      washFromWingFraction2="0.5"

      CLPerDegree="0.022"
      CDPerDegree="0.05"
      CMPerDegree="-0.01"
      flapFraction="0.4"
      degreesPerControl="12"
      
      controlPerChannel1="1"
      controlPerChannel6="0.05"
    />

    <Wing
      name="Fin"
      aerofoil="SystemData/Aerofoils/Symmetric/NACA0009"
      numSections="2"
      mass="0.05"

      positionX="-0.78"
      positionY="0.0"
      positionZ="0.0"
      rotationX="90.0"
      rotationY="0.0"
      rotationZ="0.0"
      extentsX="0.14"
      extentsY="0.2"
      extentsZ="0.01"
      wingAspectRatio="2.0"
      
      CLPerDegree="0.022"
      CDPerDegree="0.05"
      CMPerDegree="-0.01"
      flapFraction="0.4"
      degreesPerControl="30"
      
      controlPerChannel2="-1"
    />


    <Fuselage
      name="Fuselage"
      mass="0.02"
      positionX="-0.15"
      positionY="0.0"
      positionZ="0.0"
      rotationX="0.0"
      rotationY="0.0"
      rotationZ="0.0"
      extentsX="1.1"
      extentsY="0.04"
      extentsZ="0.04"

      CDX="0.1"
      CDY="0.1"
      CDZ="0.1"
      render="true"
    />

    <PropellerEngine
      name="Engine"
      positionX="0.37"
      positionY="0"
      positionZ="-0.025"
      rotationY="6.0"
      
      numBlades = "2"
      radius = "0.12"
      pitch = "0.18"
      bladeChord = "0.013"
      CL0 = "0.5"
      CLPerDegree = "0.1"
      CD0="0.02"
      CDInducedMultiplier="1.0"
      stallAngle="15"
      inertia = "0.000013"
      maxTorque = "0.08"
      maxRPM = "15000.0"
      minRPM = "0.0"
      frictionTorque ="0.05"
      washRotationFraction ="0.0"
      propDiskAreaScale = "0.35"

      controlExp="0.5"
      controlRate="6"
      controlPerChannel3="1"

      audioFile="SystemData/Audio/NewMotor44100Mono.raw"
      audioStereo="0"
      audioSampleRate="44100"
      audioRadius="0.2"
      audioMinVolume="0.0"
      audioMaxVolume="2"
      audioMinFreqScale="0.25"
      audioMaxFreqScale="0.6"

      colourX="0.5"
      colourY="0.5"
      colourZ="0.5"
      colourW="0.1"
    />
    
  </Aerodynamics>

  <Geometry>
    <!-- Additional geometry on top of the wing components etc -->

    <Shape
      type="box"
      mass="0.16"
      positionX="0.4"
      positionY="0.0"
      positionZ="0.0"
      rotationX="0.0"
      rotationY="0.0"
      rotationZ="0.0"
      extentsX="0.05"
      extentsY="0.05"
      extentsZ="0.05"
      render="true"
      collide="false"
    />
  </Geometry>

  <CrashShapes>
    <CrashShape
      type="box"
      name="Prop"
      engineName="Engine"
      crashPropSpeed="30"
      positionX="0.37"
      positionY="0"
      positionZ="-0.025"
      rotationX="45"
      extentsX="0.01"
      extentsY="0.12"
      extentsZ="0.12"
      />
  </CrashShapes>

</Physics>

<Graphics>
  <Model name="Standard" model3DS="SystemData/Aeroplanes/Neoslope/Neoslope.ac" modelScale="1.0" cullBackFace="true"/>

  <ControlSurface
    name="Elevator"
    source="Tail"
    angleMultiplier="-1"
    hingePoint1X="0.192"
    hingePoint1Y="0.194"
    hingePoint1Z="0.819"
    hingePoint2X="-0.192"
    hingePoint2Y="0.194"
    hingePoint2Z="0.819"
  />

  <ControlSurface
    name="Rudder"
    source="Fin"
    angleMultiplier="-1"
    hingePoint1X="0.0"
    hingePoint1Y="-0.005"
    hingePoint1Z="0.794"
    hingePoint2X="0.0"
    hingePoint2Y="0.186"
    hingePoint2Z="0.804"
  />

  <ControlSurface
    name="FlapLeft"
    source="Left1"
    angleMultiplier="-1"
    hingePoint1X="-0.037"
    hingePoint1Y="0.0145"
    hingePoint1Z="0.093"
    hingePoint2X="-0.399"
    hingePoint2Y="0.0145"
    hingePoint2Z="0.093"
  />

  <ControlSurface
    name="AileronLeft"
    source="Left2"
    angleMultiplier="-1"
    hingePoint1X="-0.399"
    hingePoint1Y="0.0145"
    hingePoint1Z="0.093"
    hingePoint2X="-0.909"
    hingePoint2Y="0.0385"
    hingePoint2Z="0.093"
  />

  <ControlSurface
    name="FlapRight"
    source="Right1"
    angleMultiplier="1"
    hingePoint1X="0.037"
    hingePoint1Y="0.0145"
    hingePoint1Z="0.093"
    hingePoint2X="0.399"
    hingePoint2Y="0.0145"
    hingePoint2Z="0.093"
  />

  <ControlSurface
    name="AileronRight"
    source="Right2"
    angleMultiplier="1"
    hingePoint1X="0.399"
    hingePoint1Y="0.0145"
    hingePoint1Z="0.093"
    hingePoint2X="0.909"
    hingePoint2Y="0.0385"
    hingePoint2Z="0.093"
  />
</Graphics>

<Audio>
  <AirFlow
    file="SystemData/Audio/Glider22050Stereo.raw"
    sampleRate="22050"
    mMinSpeedRelAir ="5"
    mFreqScalePerSpeed=".125"
    mMinFreqScale=".9"
    mMaxFreqScale="1.2"
    mVolScalePerSpeed=".05"
    mVolPow="2"
    mMaxVolume=".3"
  />

  <AirFlow
    file="SystemData/Audio/GliderWhistle22050Stereo.raw"
    sampleRate="22050"
    mMinSpeedRelAir ="15"
    mFreqScalePerSpeed=".1"
    mMinFreqScale=".9"
    mMaxFreqScale="1.2"
    mVolScalePerSpeed=".05"
    mVolPow="2"
    mMaxVolume="1"
  />
</Audio>


