<!--
Based on Steve Lange's Ultralight Le Fish
-->

<Misc
  launchOffsetX="2.0"
  launchOffsetY="0.0"
  launchOffsetZ="0.2"
  launchSpeed="6.0"
/>
<Physics>
  <Dynamics
    wingSpan="1.5"
    wingChord="0.15"
    CMRollFromY="0.0"
    friction="0.5"
    frictionScaleX="1"
    frictionScaleY="2"
    frictionScaleZ="2"
    />

  <Aerodynamics>
    <!-- Positions are in the centre of the chord and height, but at one end of the span (so extents.y may be negative). 
    Note that X is forward, Y is left and Z is up-->

    <Wing
      name="FuselageFrontVertical"
      aerofoil="SystemData/Aerofoils/Symmetric/NACA0009"
      numSections="1"
      mass="0.03"

      positionX="0.02"
      positionY="0.0"
      positionZ="-0.05"
      roll="90"
      extentsX="0.5"
      extentsY="0.14"
      extentsZ="0.01"
      wingAspectRatio="0.3"
    />

    <Wing
      name="FuselageBackVert"
      aerofoil="SystemData/Aerofoils/Symmetric/NACA0009"
      numSections="1"
      mass="0.01"

      positionX="-0.45"
      positionY="0.0"
      positionZ="-0.01"
      rotationX="0.0"
      rotationY="0.0"
      rotationZ="0.0"
      roll="90"
      extentsX="0.5"
      extentsY="0.02"
      extentsZ="0.01"
      wingAspectRatio="0.2"
    />

    <Wing
      name="FuselageBackHor"
      aerofoil="SystemData/Aerofoils/Symmetric/NACA0009"
      numSections="1"
      mass="0.01"

      positionX="-0.45"
      positionY="-0.01"
      positionZ="0.01"
      rotationX="0.0"
      rotationY="0.0"
      rotationZ="0.0"
      extentsX="0.2"
      extentsY="0.02"
      extentsZ="0.01"
      wingAspectRatio="0.2"
    />
    
    <Fuselage
      name="Fuselage"
      mass="0.001"
      positionX="-0.45"
      positionY="0.0"
      positionZ="0.02"
      extentsX="0.5"
      extentsY="0.03"
      extentsZ="0.05"
      CDX="0.3"
      CDY="0.3"
      CDZ="0.3"
      render="true"
    />


    <Wing
      name="Mid"
      aerofoil="SystemData/Aerofoils/SemiSymmetric/SB96V"
      numSections="1"
      mass="0.04"

      positionX="-0.07"
      positionY="-0.1"
      positionZ="0.03"
      extentsX="0.28"
      extentsY="0.2"
      extentsZ="0.01"
      
      wingAspectRatio="6.7"
      groundEffect="1"
    />

    <Wing 
      name="Left1"
      aerofoil="SystemData/Aerofoils/SemiSymmetric/SB96V"
      numSections="2"
      mass="0.05"

      positionX="-0.06"
      positionY="0.1"
      positionZ="0.03"
      extentsX="0.25"
      extentsY="0.369"
      extentsZ="0.01"
      wingAspectRatio="6.7"
      groundEffect="1"
      
      CLPerDegree="0.022"
      CDPerDegree="0.01"
      CMPerDegree="-0.0126"
      flapFraction="0.25"
      degreesPerControl="20"
      
      controlPerChannel0="1"
      controlPerChannel3="-0.5"
    />

    <Wing
      name="Left2"
      aerofoil="SystemData/Aerofoils/SemiSymmetric/SB96VS"
      numSections="2"
      mass="0.05"

      positionX="-0.03"
      positionY="0.469"
      positionZ="0.03"
      extentsX="0.17"
      extentsY="0.35"
      extentsZ="0.01"
      wingAspectRatio="6.7"
      groundEffect="1"
      
      CLPerDegree="0.022"
      CDPerDegree="0.01"
      CMPerDegree="-0.0126"
      flapFraction="0.5"
      degreesPerControl="20"
      
      controlPerChannel0="1"
      controlPerChannel3="-0.5"
    />

    <Wing
      name="Right1"
      copy="Left1"
      mirror="1"
    />

    <Wing
      name="Right2"
      copy="Left2"
      mirror="1"
    />

    <Wing
      name="Tail"
      aerofoil="SystemData/Aerofoils/Symmetric/NACA0006"
      numSections="2"
      mass="0.03"

      positionX="-0.66"
      positionY="-0.26"
      positionZ="0.07"
      pitch="0.3"
      extentsX="0.12"
      extentsY="0.52"
      extentsZ="0.01"
      wingAspectRatio="6.0"
      groundEffect="1"

      washFromWingName1="Left1"
      washFromWingName2="Right1"
      washFromWingFraction1="0.6"
      washFromWingFraction2="0.6"
      
      CLPerDegree="0.0"
      CDPerDegree="0.0"
      CMPerDegree="-0"
      flapFraction="1.0"
      degreesPerControl="85"
      
      controlPerChannel1="1.0"
      controlPerChannel3="-0.0"
      controlPerAbsChannel3="-0.0"
      
      controlRate="5.0"
    />

    <Wing
      name="Fin"
      aerofoil="SystemData/Aerofoils/Symmetric/NACA0006"
      numSections="2"
      mass="0.03"

      positionX="-0.71"
      positionY="0.0"
      positionZ="-0.02"
      rotationX="90.0"
      extentsX="0.13"
      extentsY="0.27"
      extentsZ="0.01"
      wingAspectRatio="2.0"
      
      CLPerDegree="0.01"
      CDPerDegree="0.01"
      CMPerDegree="-0.0126"
      flapFraction="0.7"
      degreesPerControl="30"
      
      controlPerChannel2="-1"
    />

  </Aerodynamics>

  <Geometry>
    <!-- Additional geometry on top of the wing components etc -->

    <Shape
      type="box"
      name="NoseWeight"
      mass="0.145"
      positionX="0.34"
      positionY="0.0"
      positionZ="0.0"
      rotationX="0.0"
      rotationY="0.0"
      rotationZ="0.0"
      extentsX="0.05"
      extentsY="0.05"
      extentsZ="0.05"
      colourTopX="0"
      colourTopY="0"
      colourTopZ="0"
      colourSidesX="0.5"
      colourSidesY="0.5"
      colourSidesZ="0.5"
      colourBottomX="1.0"
      colourBottomY="1.0"
      colourBottomZ="1.0"
      render="true"
      collide="false"
    />
    <Shape
      type="box"
      name="FrontFuselage"
      mass="0.0"
      positionX="0.15"
      positionY="0.0"
      positionZ="0.03"
      rotationX="0.0"
      rotationY="45.0"
      rotationZ="0.0"
      extentsX="0.18"
      extentsY="0.05"
      extentsZ="0.18"
      render="true"
      collide="true"
    />
  </Geometry>  
</Physics>

<Graphics>
  <Model name="Swiss" model3DS="SystemData/Aeroplanes/LeFishUltralight/LeFishSwiss.ac" modelScale="1.1" cullBackFace="true"/>

  <ControlSurface
    name="Tail"
    source="Tail"
    angleMultiplier="1"
    hingePoint1X="-0.24"
    hingePoint1Y="0.06"
    hingePoint1Z="0.59"
    hingePoint2X="0.24"
    hingePoint2Y="0.06"
    hingePoint2Z="0.59"
  />

  <ControlSurface
    name="Rudder"
    source="Fin"
    angleMultiplier="-1"
    hingePoint1X="0.00"
    hingePoint1Y="-0.03"
    hingePoint1Z="0.636"
    hingePoint2X="0.0"
    hingePoint2Y="0.251"
    hingePoint2Z="0.653"
  />

  <ControlSurface
    name="AileronLeft"
    source="Left2"
    angleMultiplier="-1"
    hingePoint1X="0"
    hingePoint1Y="0.0315"
    hingePoint1Z="0.136"
    hingePoint2X="-0.747"
    hingePoint2Y="0.030"
    hingePoint2Z="0.045"
  />

  <ControlSurface
    name="AileronRight"
    source="Right2"
    angleMultiplier="1"
    hingePoint1X="0"
    hingePoint1Y="0.0315"
    hingePoint1Z="0.136"
    hingePoint2X="0.747"
    hingePoint2Y="0.030"
    hingePoint2Z="0.045"
  />
</Graphics>

<Audio>
  <AirFlow
    file="SystemData/Audio/Glider22050Stereo.raw"
    sampleRate="22050"
    mMinSpeedRelAir ="5"
    mFreqScalePerSpeed=".125"
    mMinFreqScale=".9"
    mMaxFreqScale="1.2"
    mVolScalePerSpeed=".05"
    mVolPow="2"
    mMaxVolume=".3"
  />

  <AirFlow
    file="SystemData/Audio/GliderWhistle22050Stereo.raw"
    sampleRate="22050"
    mMinSpeedRelAir ="15"
    mFreqScalePerSpeed=".1"
    mMinFreqScale=".9"
    mMaxFreqScale="1.2"
    mVolScalePerSpeed=".05"
    mVolPow="2"
    mMaxVolume="1"
  />
</Audio>


